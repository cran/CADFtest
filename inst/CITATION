citHeader("To cite CADFtest in publications use:")

citEntry(entry = "Article",
  title        = "Unit Root CADF Testing with {R}",
  author       = as.person("Claudio Lupi"),
  journal      = "Journal of Statistical Software",
  year         = "2009",
  volume = {32},
  pages = {1--19},
  number = {2},
  textVersion  =
  paste("Claudio Lupi (2009).",
        "Unit Root CADF Testing with R.",
        "Journal of Statistical Software, 32 (2), 1-19.")
)
